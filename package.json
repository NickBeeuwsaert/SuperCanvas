{
  "name": "SuperCanvas",
  "version": "1.0.0",
  "description": "HTML5 Canvas library for parsing and using SVG paths",
  "main": "dist/SuperCanvas.js",
  "scripts": {
    "test": "mocha --compilers js:buble/register",
    "uglify": "uglifyjs -o dist/SuperCanvas.min.js --source-map dist/SuperCanvas.min.map dist/SuperCanvas.js",
    "build": "rollup -c; npm run test; npm run uglify",
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NickBeeuwsaert/SuperCanvas"
  },
  "keywords": [
    "SVG",
    "HTML5",
    "Canvas"
  ],
  "author": "Nick Beeuwsaert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NickBeeuwsaert/SuperCanvas/issues"
  },
  "homepage": "https://github.com/NickBeeuwsaert/SuperCanvas",
  "devDependencies": {
    "buble": "^0.14.2",
    "mocha": "^2.5.3",
    "rollup": "^0.36.3",
    "rollup-plugin-buble": "^0.14.0",
    "uglifyjs": "^2.4.10"
  }
}
